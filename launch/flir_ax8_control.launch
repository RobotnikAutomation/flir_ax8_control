<?xml version="1.0"?>
<launch>
  <arg name="ip_address" default="192.168.0.185" />

  <!-- start FlirAx8RemoteControl node -->
  <node name="flir_ax8_control" pkg="flir" type="flir_ax8_control_node.py" output="screen" respawn="false">
      <param name="ip_address" value="$(arg ip_address)"/>
  </node>

</launch>
